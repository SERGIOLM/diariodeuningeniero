/* CONTAINERS */		
#wrapper		{	margin-top:0px; width: 100%; overflow: hidden; height: 150px; position: relative; 								}
#logo			{ 	width: 100%; text-align :left; background-color:black; width:100%;														}
	img.logo		{	margin-top:50px;}
				 
#wrapper2		{	margin-top:-20px; overflow: hidden; height: 250px; position: relative; vertical-align: baseline;	}
#wrapper3		{	margin-top:-20px; width: 100%; overflow: hidden; height: 120px; position: relative; vertical-align: baseline;	}

#topnav 		{	top:0px; position: absolute; left:0; right:0; max-width:100%; width:90%; background:black;    				} 
	#topnav ul {	list-style:none;				}	 
	#topnav > ul 	{	display:table; width:100%; background:#000; position:absolute; 												} 
	#topnav > ul li {	display:table-cell;			}
 
		/*Sub-menu*/
		#topnav > ul > li:hover > ul 	{	display:block; height:100%;	position: absolute;											}
 
		#topnav > ul > li > ul 			{	display:block; position:absolute; background:#000; left:50px; right:0;
											overflow:hidden; height:0%; transition: all .3s ease;									}
 
		#topnav > ul li a 				{	color:#fff; display:block; line-height:20px; padding:20px; position: relative; 
											text-align:center; text-decoration:none; transition: all .3s ease; 						}
			 
			#topnav > ul > li > ul > li a:hover {	background:#5da5a2;																}			 
			#topnav > ul > li > a span 			{	background:#174459; display:block; height:100%; width:100%; left:0;
													position:absolute; top:-55px; transition: all .3s ease; 						}			 
			#topnav > ul > li > a span .icon	{	display:block; line-height:60px; 												}
			 
			#topnav > ul > li > a:hover > span {	top:0;																			}
 
			/*Colores*/
			#topnav ul li a .primero 			{ 	background:#0e5061; z-index: 0;													}			 
			#topnav ul li a .segundo			{	background:#003300;																}			 
			#topnav ul li a .tercero 			{	background:#3D3D3D;																}			 
			#topnav ul li a .cuarto 			{	background:#CC0066;																}			 
			#topnav ul li a .quinto 			{	background:#009999;																}			
			#topnav ul li a .sexto 				{	background:#994D00;																}			
			#topnav ul li a .septimo 			{	background:#721422;																}			
			#nav ul li a .octavo 				{	background:#57A639;																}
			#nav ul li a .noveno 				{	background:#B88A00;																}

