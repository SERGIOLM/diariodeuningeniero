html, body 			{ margin: 0; padding: 0; border: 0; background: transparent; font-size:10px; margin-left:5%; 	width:1200px;		}
				
div, span, article, aside, footer, hgroup, nav, section,
h1, h2, h3, h4, h5, h6, p, blockquote, a, ol, ul, li,
tr, th, td, tbody, tfoot, thead {
	margin: 0; padding: 0; border: 0; vertical-align: baseline; background: transparent;									}
img					{ margin:0; padding:0; border:0;	}

tbody, tfoot, thead {	margin: 0; padding: 0; border: 0; vertical-align: baseline; background: transparent;				}
	
table, tr, th, td, 	{ margin: 20 px; padding: 20px;
	border: 10px; border-collapse: collapse; border-spacing: 0; vertical-align: baseline; background: transparent;			}

td 					{ font-size:x-large;																					}
input, select, textarea, form, fieldset 								{	margin: 0; padding: 0; border: 0;				}

article, aside, dialog, figure, footer, header, hgroup, nav, section	{display: block;									}

h1, h2, h3, h4, h5, h6, p, li, blockquote, td, th, a, caption, em, strong, strike {
	font-family: verdana, sans-serif; font-size:100%; font-weight: normal; font-style: normal;
	line-height: 100%; text-indent: 0;	text-decoration: none; text-align: left; color: #000;								}

figcaption			{	font-family:courier-new; font-size:medium; font-style:italic; text-align: center; color: #000;		}
	
/* Global */
html	{	}
