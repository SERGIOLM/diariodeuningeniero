		function rotarImagenesUp(id)
		{
			if(index0==13) index=12;
			if(index1==9) index1=8;
			switch(id) {
				case '0':
					break;
				case 'Paris':
					ciudad=1;
					break;
				case 'Toulouse':
					ciudad=2;
					break;
			}
			if(ciudad==1)
			{			
				document.getElementById("imagen").src=imagenes_Paris[index0];
				document.getElementById("imagen").title=title_image_Paris[index0];
				index0++;
			}
			if(ciudad==2)
			{
				document.getElementById('imagen').src=imagenes_Toulouse[index1];
				document.getElementById("imagen").title=title_image_Toulouse[index1];
				index1++;
			}
		}

		function rotarImagenesDown(id)
		{
			if(index0==0) index0=1;
			if(index1==0) index1=1;
			if(ciudad==1)
			{
				index0--;
				document.getElementById("imagen").src=imagenes_Paris[index0];
				document.getElementById("imagen").title=title_image_Paris[index0];
			}
			if(ciudad==2)
			{
				index1--;
				document.getElementById("imagen").src=imagenes_Toulouse[index1];
				document.getElementById("imagen").title=title_image_Toulouse[index1];
			}
