	<script type="text/javascript">
	

    /*Array con las imagenes que se iran mostrando en la web*/
    var imagenes=new Array(
        'http://i235.photobucket.com/albums/ee43/Sergio22/banners/banner_guada_original.png',
        'http://i235.photobucket.com/albums/ee43/Sergio22/banners/banner_guada_flash.png',
        'http://i235.photobucket.com/albums/ee43/Sergio22/banners/banner_guada_poster.png',
        'http://i235.photobucket.com/albums/ee43/Sergio22/banners/banner_guada_mosaico.png'        
    );
	/*Funcion para cambiar la imagen*/
	function mostrar_imagen( id )
	{
		$( '#Guada' ).attr( 'src', 'http://i235.photobucket.com/albums/ee43/Sergio22/guada/ciudad/' + id + '.png');
		$( '#imagen_mapa' ).attr( 'src', 'http://i235.photobucket.com/albums/ee43/Sergio22/guada/ciudad/' + id + '.png');
	}
	function esconder_imagen()
	{
		$( '#Guada' ).attr( 'src', 'http://i235.photobucket.com/albums/ee43/Sergio22/guada/ciudad/guada_mapa.png');
	}

  
    function rotarImagenes()
    {
        // obtenemos un numero aleatorio entre 0 y la cantidad de imagenes que hay
        var index=Math.floor((Math.random()*imagenes.length));
 
        // cambiamos la imagen
        document.getElementById("imagen").src=imagenes[index];
    }
    onload=function()
    {
        // Cargamos una imagen aleatoria
        rotarImagenes();
 
        // Indicamos que cada 5 segundos cambie la imagen
        setInterval(rotarImagenes,5000);
    }
	function cargar_imagenes( imagenes ) {
	    $( imagenes ).each( function () {
	        $( '<img/>' )[ 0 ].src = this;
	    });
	}
	cargar_imagenes([
	        'http://i235.photobucket.com/albums/ee43/Sergio22/guada/provincia/comarcas_guada.png',
	]);
	</script>
