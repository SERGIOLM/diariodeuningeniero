		/*Funcion para cambiar la imagen*/
		function change_imagen( id )
		{
			if(id=='guada_provincia') rain_act=0;

			if(rain_act==0)	{
			$( '#guada' ).attr( 'src', 'http://i235.photobucket.com/albums/ee43/Sergio22/guada/provincia/' + id + '.png');
